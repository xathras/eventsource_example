<%= form_for @form_object, url: priced_item_scheduled_updates_path(priced_item_id: params[:priced_item_id]) do |f| %>
  <%= f.label :effective_date %>
  <%= f.text_field :effective_date, class: 'date-field' %><br>
  <%= f.label :title %><br>
  <%= f.text_area :title %>
  <br>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
  <br>
  <%= f.label :amount %><%= f.text_field :amount %><br>
  <label><%= f.check_box :taxed %>Taxed?</label>
  <br>
  <%= f.submit %>
<% end %>
